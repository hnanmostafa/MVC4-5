@using Demo.DataAccessLayer.Models
@model Employee
@{
    ViewData["Title"] = "Details";
}

<h1>@Model.Name Department Details</h1>


    <div class="form-group">
        <label asp-for="Age"></label>
        <input readonly class="form-control" asp-for="Age" />
    </div>
    <div class="form-group">
        <label asp-for="Name"></label>
        <input readonly class="form-control" asp-for="Name" />
    </div>
    <div class="form-group">
        <label asp-for="Address"></label>
        <input readonly class="form-control" asp-for="Address" />
    </div>

    <div>
        <a asp-action="Edit" class="btn btn-success" asp-route-id="@Model.Id">Edit</a>
        <a asp-action="Index" class="btn btn-secondary">Back</a>
    </div>